EAWeb.WebsiteForm.AfterLoad = function(data) {
    //Disable the submit button when the form submits.
    if (data) {
        //find the area of the page in question
        var websiteFormArea = $(data.request.customData);
        //find the form
       	websiteFormArea.find('form').each(function () {
       	    //Listen for the EAWeb.Submit event being raised that indicates the validation has completed.
       	    $(this).on("EAWeb.Submit", function() {
       	        $(this).find('input[type="submit"]').prop('disabled', true);
       	    });
	    });
    }
}
