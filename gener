<script>
$(document).ready(function() {
    $( ".head-nav .menu li.level1 .dropdown-panel" ).each(function( index ) {
    
   var maxHeight = -1;

    $(this).children('div.PANELLINK').each(function() {
     maxHeight = maxHeight > $(this).actual('height') ? maxHeight : $(this).actual('height');
   });

    $(this).children('div.PANELLINK').each(function() {
     $(this).height(maxHeight);
   });    
    
      var numItems = $(this).children('div.PANELLINK').length;
      if (numItems == 1) {
        $(this).children('div.PANELLINK').addClass("medium-12");
        $(this).removeClass("mega-menu");
        $(this).closest('li').addClass('smallMenu');
        
      }
      else if (numItems == 2) {
        $(this).children('div.PANELLINK').addClass("medium-6");
        $(this).removeClass("mega-menu");
        $(this).closest('li').addClass('smallMenu');
      }
      else if (numItems == 3) {
        $(this).children('div.PANELLINK').addClass("medium-4");
        if ($(this).find(".panel-title").length > 0){ 
           $(this).children('div.PANELLINK').last().addClass("INFO-PANEL");
        }
      }
      else if (numItems == 4) {
        $(this).children('div.PANELLINK').addClass("medium-3");
        if ($(this).find(".panel-title").length > 0){ 
           $(this).children('div.PANELLINK').last().addClass("INFO-PANEL");
        }
        
      }
      $(this).children('div.CMR1').addClass("medium-6");
      $(this).children('div.CMR2').addClass("medium-6");
    });
    $(".side-nav .Mega > a").click(function(e) {
        e.preventDefault();
        $(this).next(".dropdown-panel").addClass("active");
    });
    $(".side-nav .Mega .backPanel").click(function(e) {
        e.preventDefault();
        $(this).parent(".dropdown-panel").removeClass("active");
    });
});



$(".mobile-account").click(function(e) {
    e.preventDefault();
    $(".acct-menu-mob").slideToggle();
    $(this).toggleClass("active");
});

</script>
 
<script>
$(window).load(function() {
$(".OFFERS .dropdown-panel").addClass("mega-menu");
$(".OFFERS").removeClass("smallMenu");
var maxHeight = -1;
$('.OFFERS .dropdown-panel .PANELLINK').each(function() {
    maxHeight = maxHeight > $(this).actual('height') ? maxHeight : $(this).actual('height');
    $(this).height(maxHeight);
   });
});
</script>
