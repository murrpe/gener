EAWeb.User.AfterSignOut = function(result) {
    if (document.referrer.indexOf("order-enquiry") > 0 || document.referrer.indexOf("account-management") > 0)
    {
        window.location.replace("/");
    }
    else
    {
        location.reload();
    }
}
